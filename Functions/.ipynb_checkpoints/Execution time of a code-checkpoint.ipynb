   "source": [
    "#Calculates running time\n",
    "from time import perf_counter\n",
    "def timer():\n",
    "    start_time = perf_counter()\n",
    "    def poll():\n",
    "        return perf_counter() - start_time\n",
    "    return poll"
   ]
  },
